generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Badge {
  id     Int        @id @default(autoincrement())
  starId Int
  count  Int        @default(0)
  isLock Boolean    @default(true)
  Star   Star_Habit @relation("BadgeToStar", fields: [starId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "badge_habit_id_fk")
}

model Star_Habit {
  id        Int     @id(map: "Habit_pkey") @default(autoincrement())
  isCheck   Boolean @default(false)
  desc      String? @db.VarChar(100)
  title     String  @db.VarChar(100)
  day       Int?
  starName  String? @db.VarChar(50)
  starTitle String? @db.VarChar(100)
  introduce String? @db.VarChar(500)
  Badge     Badge[] @relation("BadgeToStar")
}
